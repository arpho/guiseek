{
  "version": 1,
  "projects": {
    "web-app": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/web-app",
      "sourceRoot": "apps/web-app/src",
      "prefix": "guiseek",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/web-app",
            "index": "apps/web-app/src/index.html",
            "main": "apps/web-app/src/main.ts",
            "polyfills": "apps/web-app/src/polyfills.ts",
            "tsConfig": "apps/web-app/tsconfig.app.json",
            "aot": false,
            "assets": [
              "apps/web-app/src/favicon.ico",
              "apps/web-app/src/assets",
              {
                "glob": "**/*",
                "input": "./libs/shared/src/assets/apps/web-app/",
                "output": "./assets"
              }
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/src/styles/apps/web-app"]
            },
            "styles": [
              "apps/web-app/src/styles.scss",
              "libs/shared/src/assets/fontawesome/css/fontawesome-all.min.css"
            ],
            "scripts": [
              "libs/shared/src/assets/apps/web-app/js/classie.js",
              "libs/shared/src/assets/apps/web-app/js/jquery.min.js",
              "libs/shared/src/assets/apps/web-app/js/jquery.scrollex.min.js",
              "libs/shared/src/assets/apps/web-app/js/jquery.scrolly.min.js",
              "libs/shared/src/assets/apps/web-app/js/browser.min.js",
              "libs/shared/src/assets/apps/web-app/js/breakpoints.min.js",
              "libs/shared/src/assets/apps/web-app/js/util.js",
              "libs/shared/src/assets/apps/web-app/js/main.js"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/frontend/environment.ts",
                  "with": "environments/frontend/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web-app:build",
            "proxyConfig": "apps/web-app/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-app:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/web-app/tsconfig.app.json", "apps/web-app/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**", "!apps/web-app/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web-app/jest.config.js",
            "tsConfig": "apps/web-app/tsconfig.spec.json",
            "setupFile": "apps/web-app/src/test-setup.ts"
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/apps/web-app/server",
            "main": "apps/web-app/src/main.server.ts",
            "tsConfig": "apps/web-app/tsconfig.server.json",
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/src/styles/apps/web-app"]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "environments/frontend/environment.ts",
                  "with": "environments/frontend/environment.prod.ts"
                }
              ],
              "sourceMap": false,
              "optimization": {
                "scripts": false,
                "styles": true
              }
            }
          }
        }
      }
    },
    "web-app-e2e": {
